            }
          } else {
            console.log('Runtime: silent refresh rejected by Cognito:', tokens.error || 'unknown');
          }
        } catch (e) {
          console.error('Runtime: silent refresh infrastructure error:', e.message);
        }
      }

      console.log('Runtime: no valid token (tried ' + idTokens.length + ' id_tokens, refresh failed), returning 401');
      return {
        status: '401',
        statusDescription: 'Unauthorized',
        headers: {
          'content-type': [{ key: 'Content-Type', value: 'text/plain' }],
        },
        body: 'Authentication required. Please login at the main application first.',
