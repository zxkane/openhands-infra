          ];

          // Update refresh_token if rotated
          if (tokens.refresh_token) {
            const refreshTokenExpiry = new Date(Date.now() + REFRESH_TOKEN_EXPIRY_MS).toUTCString();
            setCookies.push({
              key: 'Set-Cookie',
              value: \`refresh_token=\${tokens.refresh_token}; Domain=\${CONFIG.cookieDomain}; Path=/; Expires=\${refreshTokenExpiry}; HttpOnly; Secure; SameSite=None\`,
            });
          }

          // Inject verified user information
          delete request.headers['x-cognito-user-id'];
          delete request.headers['x-cognito-email'];
          delete request.headers['x-cognito-email-verified'];

          request.headers['x-cognito-user-id'] = [{
            key: 'X-Cognito-User-Id',
            value: payload.sub
          }];
          request.headers['x-cognito-email'] = [{
            key: 'X-Cognito-Email',
            value: payload.email || ''
          }];
          request.headers['x-cognito-email-verified'] = [{
            key: 'X-Cognito-Email-Verified',
            value: String(payload.email_verified || false)
          }];

          // Return request with new cookies set
          // Note: We use a redirect to set cookies and then proceed
          // This is necessary because Lambda@Edge viewer-request can't both
          // set cookies AND forward the request in a single response
          const currentUrl = \`https://\${host}\${uri}\${request.querystring ? '?' + request.querystring : ''}\`;
          return {
            status: '302',
            statusDescription: 'Found',
            headers: {
              location: [{ key: 'Location', value: currentUrl }],
              'set-cookie': setCookies,
            },
          };
        } else {
          console.error('Silent refresh: new id_token failed signature verification');
        }
      }
    } catch (e) {
      console.error('Silent refresh infrastructure error:', e.message);
    }
  }

  if (idTokens.length > 0) {
    console.log('All ' + idTokens.length + ' id_token cookies failed verification, redirecting to login');
  }

  // No valid token and refresh failed, redirect to login
  return {
    status: '302',
    statusDescription: 'Found',
    headers: {
      location: [{ key: 'Location', value: buildLoginUrl(request) }],
    },
  };
};

