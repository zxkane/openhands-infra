              request.uri = '/runtime/' + runtime.convId + '/' + runtime.port + uri;

              // For runtime requests, we need to return a redirect to set cookies
              // because we can't both set cookies and forward the request
              const currentUrl = \`https://\${host}\${uri}\${request.querystring ? '?' + request.querystring : ''}\`;
              return {
                status: '302',
                statusDescription: 'Found',
                headers: {
                  location: [{ key: 'Location', value: currentUrl }],
                  'set-cookie': setCookies,
                },
              };
