// Runtime subdomain parsing - {port}-{cid}.runtime.{subdomain}.{domain}
function parseRuntimeSubdomain(host) {
  // Match: {port}-{convId}.runtime.openhands.example.com
  const match = host.match(/^(\\d+)-([a-f0-9]{32})\\.runtime\\./);
  if (match) {
